{
    "optimizer": "SGD_Adam",
    "batch_size": 128,
    "lr": 0.1,
    "connectivities_lr" : 0.001,
    "chunks" : [64, 64, 64, 128, 128, 128, 128, 256, 256, 256, 256, 512, 512, 512, 512],
    "architecture" : "binmatr2_resnet18",
    "width_mul" : 1,
    "weight_decay" : 1e-4,
    "connectivities_l1" : 2e-6,
    "connectivities_l1_all" : true,
    "if_fully_connected" : false,
    "use_pretrained_resnet" : false,
    "input_size" : "default",
    "freeze_all_but_conns" : false,
    "if_enable_bias" : true,
    "metric_type" : "ACC",
    "weighted_ce" : "unweighted",
    "dataset": "cifar10_singletask",
    "tasks": ["all"],
    "scales": {"all":1.0}
}